/*	AS3 Port of Daniel Shiffmans MostPixelsEver - client library		AirTestTwo	Test the TCPClient. Basic ini read, and connect, tracing messages.		*/package mpe.examples {		import mpe.client.TCPClient;	import mpe.client.MpeDataListener;		import flash.display.MovieClip;	import flash.utils.getTimer;		import com.gskinner.utils.Rndm; 		public class AirTestTwo extends MovieClip implements MpeDataListener{				private var mpe:TCPClient;		public function AirTestTwo(){			trace("AirTestTwo:" + getTimer());							var ini_xml:XML = <mpe id="1" server="localhost" port="9002" localScreenSize="200,400" localLocation="400,0" masterDimensions="800,400"	debug="1"/>			mpe = new TCPClient(ini_xml, this);						mpe.start();		}				public function frameEvent(c:TCPClient):void {			// callback method from client?			// change the screen rerender.			trace("AirTestTwo:frameEvent:" + c + " : " + getTimer());		}			}}